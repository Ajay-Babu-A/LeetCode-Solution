    "class Solution(object):\n",
    "    def getRow(self, rowIndex):\n",
    "        row = [1] * (rowIndex+1)\n",
    "        for i in range(1, rowIndex):\n",
    "            \n",
    "            for j in range(i,0,-1):\n",
    "                row[j] += row[j-1]\n",
    "        return row"
   ]
  }
