   "source": [
    "class Solution(object):\n",
    "    def findMaxAverage(self, nums, k):\n",
    "        max_sum = current_sum = sum(nums[:k])\n",
    "        for i in range(len(nums)-k):\n",
    "            current_sum += nums[i+k]-nums[i]\n",
    "            max_sum = max(current_sum, max_sum)\n",
    "        return float(max_sum)/k"
   ]
  }
