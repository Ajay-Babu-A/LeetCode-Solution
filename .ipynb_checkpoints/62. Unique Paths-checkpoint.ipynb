   "source": [
    "class Solution(object):\n",
    "    def uniquePaths(self, m, n):\n",
    "        row = [1]*n\n",
    "        for i in range(m-1):\n",
    "            new_row = [1]*n\n",
    "            for j in range(n-2,-1,-1):\n",
    "                new_row[j] = new_row[j+1] + row[j]\n",
    "            row = new_row\n",
    "        return row[0]"
   ]
  }
