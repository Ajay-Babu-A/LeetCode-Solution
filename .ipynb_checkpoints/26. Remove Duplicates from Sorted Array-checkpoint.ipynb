    "class Solution(object):\n",
    "    def removeDuplicates(self, nums):\n",
    "        i,j = 0,1\n",
    "        while(i<=j and j<len(nums)):\n",
    "            if(nums[i]==nums[j]):\n",
    "                j += 1\n",
    "            else:\n",
    "                nums[i+1] = nums[j]\n",
    "                i += 1\n",
    "        k = i+1\n",
    "        return k"
   ]
  }
