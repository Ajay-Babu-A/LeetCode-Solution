   "source": [
    "class Solution(object):\n",
    "    def sumOfSquares(self, nums):\n",
    "        n = len(nums)\n",
    "        res=0\n",
    "        for i in range(n):\n",
    "            if n%(i+1)==0:\n",
    "                res += nums[i]*nums[i]\n",
    "        return res"
   ]
  }
