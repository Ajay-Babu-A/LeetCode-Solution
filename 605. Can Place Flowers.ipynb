    "class Solution(object):\n",
    "    def canPlaceFlowers(self, flowerbed, n):\n",
    "        f = [0] + flowerbed + [0]\n",
    "        for i in range(1, len(f)-1):\n",
    "            if f[i]==0 and f[i-1]==0 and f[i+1]==0:\n",
    "                f[i]=1\n",
    "                n -=1\n",
    "        return n<=0"
   ]
  }
